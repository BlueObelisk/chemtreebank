                  <amount role="VOLUME">
                    <scalar dataType="xsd:double" units="units:millilitre">9.5</scalar>
                  </amount>
                </molecule>
              </NN>
              <CC sf="and" role="CC">and</CC>
              <JJ sf="fuming">fuming</JJ>
              <NN sf="HNO3 ( 13 mL )" role="MOLECULE">
