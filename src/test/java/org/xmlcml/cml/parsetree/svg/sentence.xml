<?xml version="1.0" encoding="UTF-8"?>
<cml:cml xmlns:cml="http://www.xml-cml.org/schema"
	xmlns:units="http://www.xml-cml.org/units">
	<cml:action role="add">
		<cml:moleculeList role="mixture">
			<cml:molecule>
				<cml:name>2-bromo-3-methoxy-6-methyl-pyridine N-oxide 124C</cml:name>
		        <cml:formula inline="[O-][n+]1c(Br)c(OC)ccc1C" convention="SMILES"/>
				<cml:amount role="MASS">
					<cml:scalar dataType="xsd:double" units="units:g">7.0</cml:scalar>
				</cml:amount>
				<cml:amount role="AMOUNT">
					<cml:scalar dataType="xsd:double" units="units:mmol">32.4
					</cml:scalar>
				</cml:amount>
			</cml:molecule>
			<cml:molecule>
				<cml:formula concise="H 2 O 4 S 1">
					<cml:atomArray elementType="H O S" count="2.0 4.0 1.0" />
				</cml:formula>
				<cml:name>conc. H2SO4</cml:name>
				<cml:amount role="VOLUME">
					<cml:scalar dataType="xsd:double" units="units:mL">9.5</cml:scalar>
				</cml:amount>
			</cml:molecule>
		</cml:moleculeList>
		<cml:moleculeList>
			<cml:molecule>
				<cml:formula concise="H 2 O 4 S 1">
					<cml:atomArray elementType="H O S" count="2.0 4.0 1.0" />
				</cml:formula>
				<cml:name>H2SO4</cml:name>
				<cml:amount role="VOLUME">
					<cml:scalar dataType="xsd:double" units="units:mL">9.5</cml:scalar>
				</cml:amount>
			</cml:molecule>
			<cml:molecule>
				<cml:formula concise="H 1 N 1 O 3">
					<cml:atomArray elementType="H N O" count="1.0 1.0 3.0" />
				</cml:formula>
				<cml:name>HNO3</cml:name>
				<cml:amount role="VOLUME">
					<cml:scalar dataType="xsd:double" units="units:mL">13.0
					</cml:scalar>
				</cml:amount>
			</cml:molecule>
		</cml:moleculeList>
	</cml:action>

	<cml:action role="wait">
		<cml:parameter dictRef="cml:temp">
			<cml:scalar units="unit:celsius" dataType="xsd:double">60</cml:scalar>
		</cml:parameter>
		<cml:parameter dictRef="cml:time">
			<scalar dataType="xsd:double" units="units:minute">30.0</scalar>
		</cml:parameter>
	</cml:action>
